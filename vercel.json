{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.nxgenlowcode.biz" }],
      "destination": "https://nxgenlowcode.biz/:1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(html)",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate, max-age=0, s-maxage=0" }
      ]
    }
  ]
}

