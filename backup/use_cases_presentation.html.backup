        .slide-title {
            font-size: 1.8rem;
        }

        .slide-content {
            padding: 25px 15px;
        }
    }

    /* Animations */
    @keyframes fadeInUp {
        from {
            opacity: 0;
            transform: translateY(30px);
        }
        to {
            opacity: 1;
            transform: translateY(0);
        }
    }

    .slide-content {
         animation: fadeInUp 0.8s ease forwards;
     }

     /* File card styles from file_archive.html */
     .files-grid {
         display: grid;
         grid-template-columns: repeat(auto-fit, minmax(350px, 1fr));
         gap: 30px;
         margin-top: 40px;
     }

     .file-card {
         background: rgba(255, 255, 255, 0.05);
         backdrop-filter: blur(10px);
         border-radius: 20px;
         padding: 30px;
         transition: all 0.3s ease;
         border: 1px solid rgba(255, 255, 255, 0.1);
         text-decoration: none;
         color: inherit;
         display: block;
         position: relative;
         overflow: hidden;
     }

     .file-card::before {
         content: '';
         position: absolute;
         top: 0;
         left: -100%;
         width: 100%;
         height: 100%;
         background: linear-gradient(90deg, transparent, rgba(178, 224, 255, 0.1), transparent);
         transition: left 0.5s;
     }

     .file-card:hover::before {
         left: 100%;
     }

     .file-card:hover {
         transform: translateY(-5px);
         background: rgba(255, 255, 255, 0.08);
         border-color: rgba(178, 224, 255, 0.3);
         box-shadow: 0 20px 40px rgba(0, 0, 0, 0.3);
     }

     .file-title {
         font-size: 1.4rem;
         font-weight: 600;
         margin-bottom: 15px;
         color: #ffffff;
     }

     .file-description {
         font-size: 12px;
         font-family: 'JetBrains Mono', monospace;
         color: rgba(255, 255, 255, 0.7);
         line-height: 1.6;
         margin-bottom: 20px;
     }

     .file-meta {
         display: flex;
         justify-content: space-between;
         align-items: center;
         font-size: 0.9rem;
         color: rgba(255, 255, 255, 0.6);
     }

     .file-size {
         background: rgba(25, 233, 124, 0.2);
         color: #19e97c;
         padding: 5px 12px;
         border-radius: 15px;
         font-size: 0.8rem;
         font-weight: 500;
     }

     .back-button {
         display: inline-block;
         background: linear-gradient(135deg, #19e97c, #b2e0ff);
         color: #0a0a0a;
         padding: 15px 30px;
         border-radius: 50px;
         text-decoration: none;
         font-weight: 600;
         transition: all 0.3s ease;
         margin-bottom: 40px;
     }

     .back-button:hover {
         transform: translateY(-2px);
         box-shadow: 0 10px 30px rgba(25, 233, 124, 0.3);
     }

     /* Animation delays for file cards */
     .file-card:nth-child(1) { animation-delay: 0.1s; }
     .file-card:nth-child(2) { animation-delay: 0.2s; }
     .file-card:nth-child(3) { animation-delay: 0.3s; }
     .file-card:nth-child(4) { animation-delay: 0.4s; }
     .file-card:nth-child(5) { animation-delay: 0.5s; }
     .file-card:nth-child(6) { animation-delay: 0.6s; }
     .file-card:nth-child(7) { animation-delay: 0.7s; }
     .file-card:nth-child(8) { animation-delay: 0.8s; }
     .file-card:nth-child(9) { animation-delay: 0.9s; }
   </style>
</head>
