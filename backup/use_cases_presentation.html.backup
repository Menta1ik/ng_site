  <style>
    * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
    }

    body {
        font-family: 'Inter', sans-serif;
        background: linear-gradient(135deg, #0a0a0a 0%, #1a1a2e 50%, #16213e 100%);
        color: #ffffff;
        min-height: 100vh;
        overflow-x: hidden;
        position: relative;
    }

    /* Particles background */
    #particles-bg {
        position: fixed;
        top: 0;
        left: 0;
        width: 100vw;
        height: 100vh;
        z-index: 0;
        pointer-events: none;
    }

    .presentation-container {
        position: relative;
        z-index: 1;
        width: 100%;
        min-height: 100vh;
        padding-top: 80px;
    }

    .slide {
        width: 100%;
        min-height: calc(100vh - 80px);
        position: relative;
        opacity: 0;
        display: none;
        transition: opacity 0.5s ease-in-out;
        flex-direction: column;
        justify-content: center;
        align-items: center;
        text-align: center;
        padding: 20px;
        margin-bottom: 40px;
    }

    .slide.active {
        opacity: 1;
        display: flex;
    }

    .slide-content {
        background: linear-gradient(135deg, 
            rgba(255, 255, 255, 0.1) 0%, 
            rgba(255, 255, 255, 0.05) 50%, 
            rgba(255, 255, 255, 0.02) 100%);
        border: 1px solid rgba(255, 255, 255, 0.2);
        border-radius: 25px;
        padding: 40px;
        max-width: 1200px;
        width: 100%;
        box-shadow: 
            0 25px 50px rgba(0, 0, 0, 0.4),
            0 0 0 1px rgba(255, 255, 255, 0.1),
            inset 0 1px 0 rgba(255, 255, 255, 0.2);
        backdrop-filter: blur(20px) saturate(180%);
        -webkit-backdrop-filter: blur(20px) saturate(180%);
        max-height: calc(100vh - 200px);
        overflow-y: auto;
        margin: 20px 0;
        text-align: left;
        position: relative;
    }
    
    .slide-content::before {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background: linear-gradient(135deg, 
            rgba(255, 255, 255, 0.1) 0%, 
            transparent 50%, 
            rgba(25, 233, 124, 0.05) 100%);
        border-radius: 25px;
        pointer-events: none;
        z-index: -1;
    }

    .slide-title {
        font-size: 2.5rem;
        font-weight: 800;
        margin-bottom: 30px;
        background: linear-gradient(135deg, #ffffff, #b2e0ff);
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
        background-clip: text;
        text-align: center;
    }

    .slide-subtitle {
        font-size: 1.2rem;
        color: rgba(255, 255, 255, 0.8);
        margin-bottom: 40px;
        text-align: center;
    }

    .section-title {
        font-size: 1.3rem;
        font-weight: 600;
        color: #19e97c;
        margin: 25px 0 15px 0;
        border-bottom: 2px solid rgba(25, 233, 124, 0.3);
        padding-bottom: 8px;
    }

    .slide-btns {
        display: grid;
        grid-template-columns: repeat(4, 1fr);
        gap: 20px;
        justify-content: center;
        align-items: center;
        margin: 40px 0 0 0;
        max-width: 1000px;
        width: 100%;
    }

    .slide-btn {
        min-height: 120px;
        background: rgba(255, 255, 255, 0.05);
        color: #ffffff;
        border-radius: 20px;
        padding: 20px 15px;
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        border: 1px solid rgba(255, 255, 255, 0.1);
        cursor: pointer;
        font-size: 0.9rem;
        font-weight: 500;
        transition: all 0.3s ease;
        position: relative;
        overflow: hidden;
        backdrop-filter: blur(10px);
        text-align: center;
    }

    .slide-btn::before {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background: linear-gradient(135deg, rgba(25, 233, 124, 0.1), rgba(178, 224, 255, 0.1));
        opacity: 0;
        transition: opacity 0.3s ease;
        border-radius: 20px;
    }

    .slide-btn:hover::before {
        opacity: 1;
    }

    .slide-btn:hover {
        transform: translateY(-5px);
        border-color: rgba(25, 233, 124, 0.3);
        box-shadow: 0 15px 35px rgba(0, 0, 0, 0.3);
    }
    
    .slide-btn span {
        font-size: 2rem;
        margin-bottom: 8px;
        z-index: 1;
        position: relative;
    }

    /* Logo styles */
    .logo-container {
        position: fixed;
        top: 20px;
        left: 20px;
        z-index: 1000;
        display: flex;
        align-items: center;
        gap: 12px;
        background: rgba(255, 255, 255, 0.05);
        backdrop-filter: blur(10px);
        padding: 10px 20px;
        border-radius: 15px;
        border: 1px solid rgba(255, 255, 255, 0.1);
    }

    .logo {
        width: 40px;
        height: 40px;
        background: linear-gradient(135deg, #b2e0ff, #19e97c);
        border-radius: 8px;
        display: flex;
        align-items: center;
        justify-content: center;
    }

    .logo-text {
        font-weight: 800;
        color: #0a0a0a;
        font-size: 1.2rem;
    }

    .logo-brand {
        font-size: 1.2rem;
        font-weight: 700;
        color: white;
    }

    .logo-subtitle {
        font-size: 0.7rem;
        font-weight: 400;
        opacity: 0.8;
        color: rgba(255, 255, 255, 0.8);
    }

    /* Navigation */
     .navigation {
         position: fixed;
         bottom: 20px;
         left: 50%;
         transform: translateX(-50%);
         z-index: 1000;
         display: flex;
         gap: 15px;
         background: rgba(255, 255, 255, 0.05);
         backdrop-filter: blur(10px);
         padding: 15px 25px;
         border-radius: 50px;
         border: 1px solid rgba(255, 255, 255, 0.1);
     }

     /* Slide counter moved to top */
     .slide-counter {
         position: fixed;
         top: 20px;
         right: 20px;
         z-index: 1000;
         background: rgba(255, 255, 255, 0.05);
         backdrop-filter: blur(10px);
         padding: 10px 20px;
         border-radius: 15px;
         border: 1px solid rgba(255, 255, 255, 0.1);
         color: rgba(255, 255, 255, 0.8);
         font-size: 0.9rem;
         font-weight: 500;
     }

    .nav-btn, .home-btn {
        background: rgba(255, 255, 255, 0.1);
        color: #ffffff;
        border: none;
        padding: 10px 15px;
        border-radius: 25px;
        cursor: pointer;
        font-size: 0.9rem;
        font-weight: 500;
        transition: all 0.3s ease;
        text-decoration: none;
        display: inline-flex;
        align-items: center;
        gap: 8px;
    }

    .nav-btn:hover, .home-btn:hover {
        background: rgba(25, 233, 124, 0.2);
        color: #19e97c;
        transform: translateY(-2px);
    }

    

    /* Lists */
    ul {
        margin: 15px 0;
        padding-left: 20px;
    }

    li {
        margin-bottom: 8px;
        line-height: 1.6;
        color: rgba(255, 255, 255, 0.9);
    }

    p {
        line-height: 1.6;
        margin-bottom: 15px;
        color: rgba(255, 255, 255, 0.9);
    }

    /* Responsive adjustments */
    @media (max-width: 768px) {
        .slide-btns {
            grid-template-columns: repeat(3, 1fr);
            gap: 15px;
        }
        
        .slide-btn {
            min-height: 100px;
            font-size: 0.8rem;
            padding: 15px 10px;
        }
        
        .slide-btn span {
            font-size: 1.5rem;
        }

        .slide-title {
            font-size: 2rem;
        }

        .slide-content {
            padding: 30px 20px;
        }

        .logo-container {
            top: 10px;
            left: 10px;
            padding: 8px 15px;
        }

        .navigation {
            bottom: 10px;
            padding: 12px 20px;
        }
    }
    
    @media (max-width: 480px) {
